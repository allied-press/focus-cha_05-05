

<div class="blog-item">

  {.section item}
  <article class="entry h-entry {.main-image?} has-main-image{.end} clear" id="article-{id}" data-item-id="{id}">

    <div class="entry-content e-content">
      {.section body}
        {@}
      {.end}
    </div>

    {.if postItemInjectCode}<div class="entry-injection">{postItemInjectCode}</div>{.end}

  </article>
  {.end}


</div>

<div id="preFooter">
  <div class="pre-footer-inner">
    <squarespace:block-field id="staffBanner" columns="12" label="Staff Banner" />
  </div>
</div>


